{
  "manifest_version": 3,
  "name": "OneTabMan",
  "version": "1.0.0",
  "description": "Enforces a single tab per browser window",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
